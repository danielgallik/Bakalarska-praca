<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
	</startup>
	<system.serviceModel>
		<services>
      <service name="Alarmee.WardManager.WardManager">
        <endpoint address="net.tcp://localhost:8002/Alarmee/WardManager" binding="netTcpBinding" contract="Alarmee.WardManager.Contracts.IWardManager" />
      </service>
      <service name="Alarmee.WardPlan.PlanDataAccess">
        <endpoint address="net.tcp://localhost:8002/Alarmee/WardPlan" binding="netTcpBinding" contract="Alarmee.WardPlan.Contract.IPlanDataAccess" />
      </service>
		</services>
    <client>
      <endpoint address="net.tcp://localhost:8001/Simulator/PumpDataAccess" binding="netTcpBinding" contract="PumpContract.IPumpDataAccess" />
      <endpoint address="net.tcp://localhost:8002/Alarmee/WardPlan" binding="netTcpBinding" contract="Alarmee.WardPlan.Contract.IPlanDataAccess" />
    </client>
	</system.serviceModel>
</configuration>