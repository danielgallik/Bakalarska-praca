<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <appSettings>
    <add key="PreAlarm" value="60"/>
  </appSettings>
  <system.serviceModel>
    <services>
      <service name="Simulator.PumpDataAccess">
        <endpoint address="net.tcp://localhost:8001/Simulator/PumpDataAccess" binding="netTcpBinding" contract="PumpContract.IPumpDataAccess" />
        <endpoint address="net.tcp://localhost:9001/Simulator/AdminPumpDataAccess" binding="netTcpBinding" contract="Simulator.IAdminPumpDataAccess" />
      </service>
    </services>
    <client>
      <endpoint address="net.tcp://localhost:9001/Simulator/AdminPumpDataAccess" binding="netTcpBinding" contract="Simulator.IAdminPumpDataAccess" />
    </client>
  </system.serviceModel>
</configuration>